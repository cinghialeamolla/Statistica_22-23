# seconda lezione

numeri <- c(1, 2, 3)
numeri

colori <- c("rosso", "giallo", "verde")
colori
# [1] "rosso"  "giallo" "verde" 

str(numeri)
#num [1:3] 1 2 3

logico <- c(T, F)
logico
#[1]  TRUE FALSE
str(logico)
#logi [1:2] TRUE FALSE

#creazione matrice
X <- matrix(c(3,0,6,0,6,9), nrow=3, ncol=2)
X
#     [,1] [,2]
#[1,]    3    0
#[2,]    0    6
#[3,]    6    9
str(X)
# num [1:3, 1:2] 3 0 6 0 6 9

